package com.petboard.repository;

import java.sql.Timestamp;

public class MemberVo {

	private int memberNumber;
	private String email;
	private String password;
	private String name;
	private String nickName;
	private String pet;
	private Timestamp joinDate;
	private int memberLevel;
	private char memberStatus;
	private char emailVerify;
	private String option;
	private String searchText;

	public MemberVo() {}
	
	public MemberVo(int memberNumber, String email, String password, String name, String nickName, String pet,
			Timestamp joinDate, int memberLevel, char memberStatus, char emailVerify) {
		super();
		this.memberNumber = memberNumber;
		this.email = email;
		this.password = password;
		this.name = name;
		this.nickName = nickName;
		this.pet = pet;
		this.joinDate = joinDate;
		this.memberLevel = memberLevel;
		this.memberStatus = memberStatus;
		this.emailVerify = emailVerify;
	}

	public int getMemberNumber() {
		return memberNumber;
	}

	public void setMemberNumber(int memberNumber) {
		this.memberNumber = memberNumber;
	}

	public String getEmail() {
		return email;
	}

	public void setEmail(String email) {
		this.email = email;
	}

	public String getPassword() {
		return password;
	}

	public void setPassword(String password) {
		this.password = password;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getNickName() {
		return nickName;
	}

	public void setNickName(String nickName) {
		this.nickName = nickName;
	}

	public String getPet() {
		return pet;
	}

	public void setPet(String pet) {
		this.pet = pet;
	}

	public Timestamp getJoinDate() {
		return joinDate;
	}

	public void setJoinDate(Timestamp joinDate) {
		this.joinDate = joinDate;
	}

	public int getMemberLevel() {
		return memberLevel;
	}

	public void setMemberLevel(int memberLevel) {
		this.memberLevel = memberLevel;
	}

	public char getMemberStatus() {
		return memberStatus;
	}

	public void setMemberStatus(char memberStatus) {
		this.memberStatus = memberStatus;
	}

	public char getEmailVerify() {
		return emailVerify;
	}

	public void setEmailVerify(char emailVerify) {
		this.emailVerify = emailVerify;
	}

	public String getOption() {
		return option;
	}

	public void setOption(String option) {
		this.option = option;
	}

	public String getSearchText() {
		return searchText;
	}

	public void setSearchText(String searchText) {
		this.searchText = searchText;
	}


}
